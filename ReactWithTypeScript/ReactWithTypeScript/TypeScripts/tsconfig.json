{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "jsx": "react",
    "outDir": "../Scripts/CompiledTS",
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types"]
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
